{
  "name": "mock-proxy-autocookie",
  "version": "0.0.2",
  "description": "local server mock and proxy tool",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && mkdir dist && ((uglifyjs src/index.js -o dist/index.js -c) & (uglifyjs src/utils.js -o dist/utils.js -c) & (uglifyjs src/mock.js -o dist/mock.js -c) & (uglifyjs src/proxy.js -o dist/proxy.js -c) & (node src/api-doc/build.js))",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangshaolong/mock-proxy-autocookie.git"
  },
  "keywords": [
    "mock",
    "proxy",
    "server"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "zhangshaolong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhangshaolong/mock-proxy-autocookie/issues"
  },
  "homepage": "https://github.com/zhangshaolong/mock-proxy-autocookie#readme",
  "dependencies": {
    "chrome-cookies-secure": "^1.3.3"
  },
  "devDependencies": {
    "service-api": "^0.1.4",
    "simplite": "0.0.2",
    "simplite-loader": "0.0.3",
    "uglify-es": "^3.3.9",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
